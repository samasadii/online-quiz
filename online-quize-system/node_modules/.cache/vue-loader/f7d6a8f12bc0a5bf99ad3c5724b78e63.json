lor: #083535;\n  font-family: \"Gill Sans\", \"Gill Sans MT\", Calibri, \"Trebuchet MS\", sans-serif;\n  margin: 20px auto 50px auto;\n  text-align: center;\n}\n.img {\n  margin-top: 150px;\n  display: block;\n  background-blend-mode: color;\n  background-color: #eef3f3;\n  height: 420px;\n  color: #083535;\n}\n.footer {\n  background-color: #eef3f3;\n  height: 200px;\n}\n\n.rightSide {\n  width: 50%;\n  display: flex;\n  flex-direction: column;\n  margin-top: 70px;\n  text-align: left;\n  height: auto;\n  font-family: \"Gill Sans\", \"Gill Sans MT\", Calibri, \"Trebuchet MS\", sans-serif;\n}\n.rightSide h3 {\n  margin-bottom: 15px;\n  font-size: 36px;\n  text-align: center;\n}\n.form-input input {\n  display: table;\n  background-color: #eef3f3;\n  border-radius: 50px !important;\n  padding-top: 10px;\n  padding-bottom: 10px;\n  width: 50%;\n  border: none !important;\n  border: 1px solid #eef3f3 !important;\n  font-size: 20px !important;\n  color: #000 !important;\n  font-weight: 400;\n  margin: 0 auto;\n  padding-left: 28px;\n  align-self: center;\n  margin-bottom: 16px;\n}\n.btn-submit {\n  display: table;\n  border-radius: 50px !important;\n  padding-top: 10px;\n  padding-bottom: 10px;\n  width: 50%;\n  border: 1px solid #083535 !important;\n  font-size: 20px !important;\n  color: #eef3f3;\n  margin: 0 auto;\n  margin-top: 50px;\n  text-align: center;\n  align-self: center;\n}\n\ninput:focus,\ntextarea:focus {\n  box-shadow: none !important;\n  border: 2px solid #083535 !important;\n  background-color: #eef3f3;\n  outline-width: 0;\n  font-weight: 400;\n}\n::placeholder {\n  font-size: 20px;\n  margin-left: 20px;\n  text-align: left;\n}\n::placeholder:focus {\n  color: #000;\n}\n.sign-up.text-center {\n  font-size: 20px;\n  color: #083535;\n  margin-bottom: 100px;\n}\n.dark-green {\n  background-color: #083535;\n}\n</style>\n"]}]}